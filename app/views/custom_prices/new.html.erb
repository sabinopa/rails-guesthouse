<h1> <%= t('views.new.personalize') %></h1>

<% if(@custom_price.errors.any?) %>
  <p><%= t('errors.messages.verify_error') %></p>
  <ul>
    <% @custom_price.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
  </ul>
<% end %>

<%= render partial: 'form', locals: { custom_price: @custom_price } %>